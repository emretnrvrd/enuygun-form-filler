{
  "name": "enuygun-form-filler",
  "description": "Enuygun Auto Form Filler",
  "version": "0.0.1",
  "manifest_version": 3,
  "background": {
    "service_worker": "serviceWorker.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ],
  "permissions": ["tabs", "activeTab", "scripting", "storage"],
  "options_ui": {
    "page": "options/main.html",
    "open_in_tab": true
  },
  "action": {
    "default_title": "Enuygun Auto Form Filler",
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/icons/16x.png",
      "32": "/icons/32x.png",
      "48": "/icons/48x.png",
      "128": "/icons/128x.png"
    }
  },
  "icons": {
    "16": "/icons/16x.png",
    "32": "/icons/32x.png",
    "48": "/icons/48x.png",
    "128": "/icons/128x.png"
  }
}